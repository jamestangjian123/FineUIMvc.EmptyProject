
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html dir="ltr" lang="en-US">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <title>顺秋综合档案管理系统</title>

    <!--- CSS --->
    <link rel="stylesheet" href="~/res/loginNew/style.css" type="text/css" />


    <!--- Javascript libraries (jQuery and Selectivizr) used for the custom checkbox --->
    <!--[if (gte IE 6)&(lte IE 8)]>
        <script type="text/javascript" src="~/res/loginNew/jquery-1.7.1.min.js"></script>
        <script type="text/javascript" src="~/res/loginNew/selectivizr.js"></script>
        <noscript><link rel="stylesheet" href="~/res/loginNew/fallback.css" /></noscript>
    <![endif]-->

</head>

<body>
    <div id="container">
        <form action="welcome.html">
            <div class="login">欢迎使用顺秋综合档案管理系统</div>
            <div class="username-text">用户名:</div>
            <div class="password-text">密码:</div>
            <div class="username-field">
                <input type="text" name="username" value="" />
            </div>
            <div class="password-field">
                <input type="password" name="password" value="" />
            </div>
            <input type="checkbox" name="remember-me" id="remember-me" /><label for="remember-me">Remember me</label>
            <div class="forgot-usr-pwd">Forgot <a href="#">username</a> or <a href="#">password</a>?</div>
            <input type="submit" name="submit" value="GO" />
        </form>
    </div>
    <div id="footer">
        顺秋综合档案管理系统&nbsp;&nbsp;当前版本: 2017.08.01</a>
    </div>
</body>
</html>

